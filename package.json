{
    "name": "keChuangYun",
    "version": "1.0.3",
    "private": true,
    "description": "科创云平台-小程序，H5",
    "apiDocPath": "http://test.portal.heimaqf.cn/v2/api-docs",
    "templateInfo": {
        "name": "default",
        "typescript": true,
        "css": "sass"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-customizable"
        },
        "cz-customizable": {
            "config": ".cz-config.js"
        }
    },
    "browser": {
        "fs": false,
        "path": false,
        "os": false
    },
    "resolutions": {
        "@types/react": "17.0.2",
        "@types/react-dom": "17.0.2"
    },
    "scripts": {
        "jscpd": "jscpd ./src/*",
        "prettier": "prettier --write ./src",
        "fix": "eslint --ext .js,.ts,.tsx,.jsx src/ --fix",
        "build:weapp": "taro build --type weapp",
        "build:swan": "taro build --type swan",
        "build:alipay": "taro build --type alipay",
        "build:tt": "taro build --type tt",
        "build:h5": "taro build --type h5",
        "build:rn": "taro build --type rn",
        "build:qq": "taro build --type qq",
        "build:jd": "taro build --type jd",
        "build:quickapp": "taro build --type quickapp",
        "dev:weapp": "npm run build:weapp -- --watch",
        "dev:swan": "npm run build:swan -- --watch",
        "dev:alipay": "npm run build:alipay -- --watch",
        "dev:tt": "npm run build:tt -- --watch",
        "dev:h5": "npm run build:h5 -- --watch",
        "dev:rn": "npm run build:rn -- --watch",
        "dev:qq": "npm run build:qq -- --watch",
        "dev:jd": "npm run build:jd -- --watch",
        "dev:quickapp": "npm run build:quickapp -- --watch",
        "swagger": "node swaggerSync && node ./gencode.js",
        "build": "npm run build:h5 && npm run build:weapp"
    },
    "browserslist": [
        "last 3 versions",
        "Android >= 4.1",
        "ios >= 8"
    ],
    "author": "",
    "dependencies": {
        "@babel/helper-environment-visitor": "^7.16.7",
        "@babel/runtime": "^7.7.7",
        "@reduxjs/toolkit": "^1.5.1",
        "@tarojs/components": "3.3.3",
        "@tarojs/react": "3.2.8",
        "@tarojs/runtime": "3.2.8",
        "@tarojs/taro": "3.2.8",
        "axios": "^0.21.1",
        "crypto-js": "^4.0.0",
        "dayjs": "^1.10.4",
        "default-passive-events": "^2.0.0",
        "echarts": "^5.2.2",
        "echarts-wordcloud": "^2.0.0",
        "js-base64": "^3.6.1",
        "numeral": "^2.0.6",
        "path": "^0.12.7",
        "postcss-px-scale": "^1.1.1",
        "react": "^17.0.0",
        "react-copy-to-clipboard": "^5.0.3",
        "react-dom": "^17.0.0",
        "react-helmet": "^6.1.0",
        "react-redux": "^7.2.4",
        "react-sticky": "^6.0.3",
        "redux": "^4.1.0",
        "redux-thunk": "^2.3.0",
        "taro-ui": "^3.0.0-alpha.10"
    },
    "devDependencies": {
        "@babel/core": "^7.8.0",
        "@commitlint/cli": "^12.1.1",
        "@commitlint/config-conventional": "^12.1.1",
        "@tarojs/mini-runner": "3.2.8",
        "@tarojs/webpack-runner": "3.2.8",
        "@types/clipboard": "^2.0.1",
        "@types/react": "^17.0.2",
        "@types/webpack-env": "^1.13.6",
        "@typescript-eslint/eslint-plugin": "^4.15.1",
        "@typescript-eslint/parser": "^4.15.1",
        "@tarojs/plugin-framework-react": "^3.4.13",
        "@types/react-dom": "^18.0.6",
        "babel-preset-taro": "3.2.8",
        "commitizen": "^4.2.3",
        "cz-conventional-changelog": "^3.3.0",
        "cz-customizable": "^6.3.0",
        "eruda": "^2.4.1",
        "eslint": "^7.23.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-config-prettier": "^6.12.0",
        "eslint-config-taro": "3.2.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.21.4",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-webpack-plugin": "^2.5.2",
        "husky": "^4.3.0",
        "lint-staged": "^10.5.4",
        "prettier": "^2.3.0",
        "stylelint": "9.3.0",
        "typescript": "^4.1.0",
        "tiff.js": "^1.0.0",
        "path-browserify": "^1.0.1",
        "crypto-browserify": "^3.12.0",
        "stream-browserify": "^3.0.0"
    }
}
